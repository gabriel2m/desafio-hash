<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
>
    <title>Hash Challenge</title>
    <paths>
        <output>public/doc</output>
    </paths>
    <version number="1.0">
        <api format="php">
            <ignore hidden="true" symlinks="true">
                <path>tests/**/*</path>
                <path>vendor/**/*</path>
                <path>var/**/*</path>
            </ignore>
        </api>
    </version>
</phpdocumentor>
